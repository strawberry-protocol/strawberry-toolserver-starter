{
  "name": "@strawberryprotocol/toolserver-starter",
  "description": "Strawberry.fun Toolserver Starter",
  "version": "1.0.0",
  "author": "Strawberry.fun Team",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "NODE_OPTIONS='--no-warnings' node ./dist/index.js",
    "dev": "tsx ./src/index.ts",
    "lint": "eslint -c .eslintrc.json *.ts",
    "lint-fix": "eslint -c .eslintrc.json *.ts --fix",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "format-check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.3.2",
    "@strawberryprotocol/str-toolserver": "0.0.1",
    "dotenv": "^16.4.7",
    "get-port": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  }
}
